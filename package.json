{
  "name": "visionbrowser",
  "version": "20.4.5",
  "description": "Simple and minimalist browser",
  "main": "main.js",
  "scripts": {
    "start": "npx electron .",
    "dist": "npx electron-builder build",
    "publish": "build --win -p never",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/BeanedTaco/vision",
  "keywords": [
    "Browser",
    "Made with Electron",
    "Vision Browser"
  ],
  "author": "The Vision Project",
  "contributors": [
    "SaturdayNightDead <danny@saturdaynightdead.xyz> (https://saturdaynightdead.xyz)",
    "trentanator09 (https://www.reddit.com/user/trentanator09)"
  ],
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.4.1",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "discord.js": "^12.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-tabs": "^0.15.0",
    "electron-updater": "^4.2.5",
    "favicon-getter": "^1.1.3",
    "v8-compile-cache": "^2.1.0"
  },
  "build": {
    "appId": "com.visionproject.vision",
    "productName": "Vision Browser",
    "asar": false,
    "publish": [
      {
        "provider": "github",
        "owner": "BeanedTaco",
        "repo": "vision"
      }
    ],
    "win": {
      "icon": "build/icon.ico",
      "publish": "github",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico",
      "license": "build/license.txt",
      "language": "2057",
      "shortcutName": "Vision Browser",
      "menuCategory": "The Vision Project"
    }
  }
}
